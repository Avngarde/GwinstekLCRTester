        <TextBox HorizontalAlignment="Left" Name="Freq4" Height="34" Margin="695,389,0,0" Text="0" TextWrapping="Wrap" FontSize="20" VerticalAlignment="Top" Width="200" Grid.Column="1"/>
        <TextBlock HorizontalAlignment="Left" Text="Częstotliwość 4:" FontSize="20" TextWrapping="Wrap" VerticalAlignment="Top" Margin="526,393,0,0" Grid.Column="1"/>
        <Border BorderThickness="1" CornerRadius="10" BorderBrush="Black" HorizontalAlignment="Left" Height="400" Margin="485,0,0,0" VerticalAlignment="Center" Width="880" Grid.Column="1"/>
